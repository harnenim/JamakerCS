  <Target Name="RemoveLangFolders" AfterTargets="PrepareForBuild">
    <ItemGroup>
      <LangFoldersToDelete Include="$(OutDir)\cs;$(OutDir)\de;$(OutDir)\es;$(OutDir)\fr;$(OutDir)\it;$(OutDir)\ja;$(OutDir)\ko;$(OutDir)\pl;$(OutDir)\pt-BR;$(OutDir)\ru;$(OutDir)\tr;$(OutDir)\zh-Hans;$(OutDir)\zh-Hant;" />
    </ItemGroup>
    <RemoveDir Directories="@(LangFoldersToDelete)" />
